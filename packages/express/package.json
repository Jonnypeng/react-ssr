  "author": "Yoshihide Shiono <shiono.yoshihide@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/saltyshiomix/react-ssr/tree/master/packages/express",
  "keywords": [
    "react",
    "reactjs",
    "react.js",
    "express",
    "expressjs",
    "ssr"
  ],
  "bugs": {
    "url": "https://github.com/saltyshiomix/react-ssr/issues",
    "email": "shiono.yoshihide@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rimraf build && run-p build:src build:types",
    "build:src": "rollup -c",
